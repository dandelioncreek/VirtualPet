let angle = 0;

let x = 50;
let y = 50;

function setup(){
  createCanvas(400, 400);
  angleMode(DEGREES);
  rectMode(CENTER);
}

function draw(){
  background(0);
  
  translate(x, y);
  rotate(angle);
  fill(255, 100, 50);
  rect(0, 0, 100, 50);
  
  fill(50, 100, 255);
  rect(300, 300, 100, 50);
  
  x = x + 2;
  angle = angle + 5;
}
